{
  "name": "states-us",
  "description": "List of US states and territories.",
  "version": "1.0.0",
  "keywords": [
    "united states",
    "usa",
    "us",
    "states",
    "territories",
    "array",
    "abbreviation",
    "abbreviations"
  ],
  "author": {
    "name": "Justin Lettau",
    "email": "me@justinlettau.com",
    "url": "https://justinlettau.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/justinlettau/states-us",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinlettau/states-us"
  },
  "bugs": {
    "url": "https://github.com/justinlettau/states-us/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint -c ./tslint.json -p ./tsconfig.json",
    "build": "tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "test": "nyc jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "posttest": "nyc report --reporter=json && codecov -f coverage/*.json",
    "format": "prettier --write **/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@justinlettau/renovate-config": "^0.1.2",
    "@justinlettau/tslint-config": "^0.1.0",
    "@types/jasmine": "^3.3.12",
    "codecov": "^3.3.0",
    "husky": "^4.0.10",
    "jasmine": "^3.4.0",
    "nyc": "^15.0.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^2.0.1",
    "standard-version": "^7.0.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  }
}
